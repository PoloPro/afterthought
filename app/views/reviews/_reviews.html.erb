
<% reviews.each do |review| %>

<blockquote>
  <%= review.content %>
  <footer>
    <% if review.anonymous %>
    Anonymous, on <em><%= review.lecture.title %></em>
    <% else %>
    <%= review.student.name %> on <em><%= review.lecture.title %></em>
    <% end %>
  </footer>
</blockquote>

<% end %>